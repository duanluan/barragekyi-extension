{
  "manifest_version": 2,
  "name": "Barrage kyi",
  "description": "彈幕姬 Chrome 插件版。",
  "version": "0.0.0.5",
  "icons": {
    "16": "images/Barrage_kyi_icon_16.png",
    "48": "images/Barrage_kyi_icon_48.png",
    "128": "images/Barrage_kyi_icon_128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/Barrage_kyi_icon_19.png",
      "38": "images/Barrage_kyi_icon_38.png"
    },
    "default_title": "Barrage kyi",
    "default_popup": "popup.html"
  },
  "permissions": [
    "notifications",
    "cookies",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "js/jquery-1.12.4.min.js",
      "js/common.js",
      "js/bilibiliCommon.js",
      "js/background.js"
    ],
    "persistent": true
  }
}
